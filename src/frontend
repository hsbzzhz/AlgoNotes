<!-- <script src="https://cdn.bootcdn.net/ajax/libs/axios/0.21.1/axios.min.js"></script> -->
<script src="http://code.jquery.com/jquery-2.1.4.min.js"></script>

<script>
    let app = new Vue({
        el: '#app',
        data: {
            reqInfo: {
                categoryLevel1: '',
                group: '',
                title: '',
                processingStatus: '',
                plinkTaskId: '',
                askId: '',
            },
            statusInfo: [],
            issueData: [],
            more_msg: '点击加载更多',
            page: 1,

            menuItemsLay1:[
                {
                    name: '最新' ,
                    id:''
                },
                {
                    name: '我的' ,
                    id:'mine'
                }
            ],
            menuItemsLay2:[
                {
                    name: '全部' ,
                    id:''
                },
                {
                    name: '功能建议' ,
                    id:'需求类问题跟踪'
                },
                {
                    name: '产品BUG' ,
                    id:'BUG类问题跟踪'
                }
            ],
            menuItemsLay3:[
                {
                    name: '全部' ,
                    id:''
                },
                {
                    name: 'ADC产品' ,
                    id:'ADC2.X问答专区(GDE版本23.X)'
                },
                {
                    name: 'GDE Store' ,
                    id:'GDE Store'
                },
                {
                    name: '智能运维' ,
                    id:'智能运维'
                },
                {
                    name: 'GDE DateGuard' ,
                    id:'disabled'
                },
                {
                    name: 'GDE 学院' ,
                    id:'disabled'
                },
                {
                    name: 'GTS低码社区' ,
                    id:'disabled'
                },
            ],
            menuItemsLay4:[
                {
                    name: '全部' ,
                    id:''
                },
                {
                    name: '未采纳' ,
                    id:'未采纳'
                },
                {
                    name: '评审中' ,
                    id:'评审中'
                },
                {
                    name: '已采纳' ,
                    id:'已采纳'
                },
                {
                    name: '已实现' ,
                    id:'已实现'
                },
            ],
        },
        mounted() {
            this.initPost();
            this.getIssueData(this.page,10);
        },
        watch: {
            reqInfo: {
                handler(val, oldVal) {
                    // console.log(`new: ${val}, old: ${oldVal}`);
                    this.getIssueData(1,10);
                },
                deep: true
            }
        },
        methods: {
            handleClickMenu1: function(item){
                // 我的/最新
                this.reqInfo.askId = item.id;
                console.log(this.reqInfo);
            },
            handleClickMenu2: function(item){
                // 建议类型
                this.reqInfo.group = item.id;
            },
            handleClickMenu3: function(item){
                // 功能分类
                this.reqInfo.categoryLevel1 = item.id;
            },
            handleClickMenu4: function(item){
                // 阶段
                this.reqInfo.processingStatus = item.id;
            },
            initPost: function () {
                let self = this;
                let url = 'http://100.95.40.138:8005/issues/status';
                console.log("======initPost执行==========");
                $.ajax({
                    type: "get",
                    url: url,
                    success: function (res) {
                        for (let i = 0; i < res.data.length; i++) {
                            if (res.data[i]['processing_status'] === '已实现') {
                                self.statusInfo.push(res.data[i]['count']);
                            }
                            if (res.data[i]['processing_status'] === '评审中') {
                                self.statusInfo.push(res.data[i]['count']);
                            }
                            if (res.data[i]['processing_status'] === '已采纳') {
                                self.statusInfo.push(res.data[i]['count']);
                            }
                        }
                    }
                });
            },
            getIssueData: function (page, size) {
                let url = `http://100.95.40.138:8005/issues/${page}/${size}`;
                $.ajax({
                    type: 'post',
                    url: url,
                    contentType:'application/json',
                    data: JSON.stringify(this.reqInfo),
                    success: res => {
                        if (res.data['records'].length > 0) {
                            if (page === 1) {
                                this.issueData = res.data['records'];
                            } else {
                                this.issueData = this.issueData.concat(res.data['records']);
                            }
                            console.log(this.issueData);
                        } else {
                            this.more_msg = '已经到底了~';
                        }
                    }
                });
            },
            loadIssue: function () {
                this.getIssueData(this.page++, 10)
            },
            jumptopage(item) {
                console.log(item);
                window.open(item);
            }
        }
    });
</script>
</body>

